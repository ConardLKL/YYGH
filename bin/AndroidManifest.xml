<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lhjl.yygh"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo1"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity 
            android:name="com.lhjl.yygh.LoginActivity"
            android:theme="@android:style/Theme.NoTitleBar" 
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity
            android:name=".MainActivity"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="main" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".KeShiActivity"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="keshi" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity
            android:name=".YishengListActivity"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="yishenglist" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
              <activity
            android:name=".TiJiaoDingdanActivity"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="tijiaodingdan" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
               <activity
            android:name=".YuYueActivity"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="yuyue" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity> 
           <activity
            android:name=".YuYueNewActivity"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="yuyuenew" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
                   <activity
            android:name=".MyyuyueListActivity"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="myyuyuelist" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity> 
                <activity
            android:name=".YuYuecontentActivity"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="yuyuecontent" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity> 
          <activity
            android:name=".BindCardActivity"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="bindcard" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity> 
        
    </application>
<!--往sdcard中写入数据的权限 -->
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
 <!--在sdcard中创建/删除文件的权限 -->
<uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"></uses-permission>
</manifest>
